    "lodash.get": "^4.4.2",
    "lodash.pick": "^4.4.0",
    "mathjax": "^3.1.2",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "node-sass": "^4.14.1",
    "node-uuid": "^1.4.8",
    "nprogress": "^0.2.0",
    "postcss-import": "^12.0.1",
    "postcss-px2rem": "^0.3.0",
    "postcss-url": "^8.0.0",
    "px2rem-loader": "^0.1.9",
    "sass-loader": "^9.0.3",
    "save": "^2.4.0",
    "screenfull": "^5.0.2",
    "swiper": "^6.1.2",
