    "lint:nofix": "vue-cli-service lint --no-fix",
    "test:unit": "vue-cli-service test:unit",
    "postinstall": "opencollective-postinstall"
  },
  "dependencies": {
    "@antv/data-set": "^0.11.7",
    "@antv/g2": "^4.1.0",
    "@dimakorotkov/tinymce-mathjax": "^1.0.6",
    "@tinymce/tinymce-vue": "^3.2.3",
    "animate.css": "^4.1.0",
    "ant-design-vue": "^1.6.0",
    "axios": "^0.20.0",
    "cli-plugin-eslint": "^0.0.1",
    "core-js": "^2.6.5",
    "echarts": "^4.7.0",
    "echarts-stat": "^1.2.0",
